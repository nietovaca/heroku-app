<!DOCTYPE html>
<html lang="en" dir="ltr">
  <head>
    <%- include('partials/header.ejs')%>
  </head>
  <body>
    <h1><%=student.name;%></h1>
    <table class="table table-hover">
      <thead>
        <tr>
          <th scope="col">Last Update</th>
          <th scope="col">Participation</th>
          <th scope="col">Notes</th>
        </tr>
      </thead>
      <tbody>
        <tr>
          <th scope="row"> </th>
            <td> <%= student.timestamps; %> </td>
            <td> <%=student.participation%> </td>
            <td class="text-sm-left"> <%=student.notes%> </td>
        </tr>
      </tbody>
</table>
  <button type="button" class="btn btn-warning" name="button"><a href="/teacherspet/" class="text-dark">Home</a></button>
  <form class="" action="/teacherspet/<%=student.id;%>?_method=DELETE" method="POST">
    <input type="submit" value="DELETE">
  </form>
  </body>
</html>
